{
    "require-dev": {
        "vimeo/psalm": "^5.15",
        "ging-dev/psalm-plugin-wordpress": "5.x-dev",
        "wp-coding-standards/wpcs": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "psalm": "psalm --no-cache"
    }
}
